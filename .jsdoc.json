{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "recurseDepth": 10,
    "sourceType": "module",
    "source": {
        "include": [ "./src" ]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "My SDK Name"
    },
    "opts": {
        "destination": "./docs",
        "encoding": "utf8",
        "recurse": true,
        "template": "./jsdoc-template"
    }
}
